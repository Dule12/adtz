{
  "name": "adtz",
  "version": "0.0.6",
  "description": "Pattern mattching, ADTs for Typescript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/Dule12/adtz.git",
  "author": "Dusan Svitlica dsvitlica@gmail.com",
  "license": "MIT",
  "private": false,
  "scripts": {
    "doc": "cross-var typedoc --name $npm_package_name --mode modules --module commonjs --excludePrivate --excludeExternals --externalPattern '**/node_modules/**' --out doc/ packages/**/ts/",
    "build": "cross-var tsc --sourceRoot $npm_package_name --listEmittedFiles --pretty --inlineSourceMap --inlineSources",
    "watch": "cross-var tsc -w --sourceRoot $npm_package_name --listEmittedFiles --pretty --inlineSourceMap --inlineSources"
  },
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "keywords": [
    "ADT",
    "Algebraic data types",
    "Discriminated union",
    "Functional programming",
    "Pattern matching",
    "Match expression",
    "Sum types",
    "Tagged unions",
    "TypeScript",
    "Collections",
    "Option",
    "Typesafe",
    "Monad"
  ],
  "dependencies": {
    "ts-node": "^8.5.4",
    "typelevel-ts": "^0.4.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "chai": "^4.2.0",
    "mocha": "^6.2.2"
  }
}
