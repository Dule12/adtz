{
    "extension": [
        ".ts"
    ],
    "include": [
        "packages"
    ],
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "watermarks": {
        "lines": [
            60,
            90
        ],
        "functions": [
            60,
            90
        ],
        "branches": [
            60,
            90
        ],
        "statements": [
            60,
            90
        ]
    },
    "statements": 60,
    "functions": 60,
    "branches": 60,
    "lines": 60
}